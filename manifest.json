{
	"manifest_version": 2,
	"name": "__MSG_extName__",
	"description": "__MSG_extDescription__",
	"short_name": "HowManyTabs",
	"version": "1.1.0",
	"icons": {
		"16": "icon/16.png",
		"48": "icon/48.png",
		"128": "icon/128.png"
